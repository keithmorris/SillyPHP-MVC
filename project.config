[project]
	description = Yet another PHP MVC Framework. This was not built to be a production MVC Framework but rather as a learning tool for me around building a more robust and flexible framework.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Keith Morris (keithmorris)
[access "refs/for/refs/*"]
	read = group user/Keith Morris (keithmorris)
	push = group user/Keith Morris (keithmorris)
	label-Code-Review = -2..+2 group user/Keith Morris (keithmorris)
	label-Verified = -1..+1 group user/Keith Morris (keithmorris)
	rebase = group user/Keith Morris (keithmorris)
	removeReviewer = group user/Keith Morris (keithmorris)
	submit = group user/Keith Morris (keithmorris)
[access "refs/heads/*"]
	read = group user/Keith Morris (keithmorris)
	create = group user/Keith Morris (keithmorris)
	push = +force group user/Keith Morris (keithmorris)
	pushMerge = group user/Keith Morris (keithmorris)
[access "refs/tags/*"]
	push = +force group user/Keith Morris (keithmorris)
	pushTag = group user/Keith Morris (keithmorris)
	pushSignedTag = group user/Keith Morris (keithmorris)
